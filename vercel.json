{
    "version": 2,
    "github": {
        "enabled": false
    },
    "builds": [
        {
            "src": "/api/main.ts",
            "use": "@vercel/node"
        },
        {
            "src": "/frontend/*",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}